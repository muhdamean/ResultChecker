    <!-- Header Area -->
    <header class="header">
        <!-- Topbar -->
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-5 col-12">
                        <!-- Contact -->
                        <ul class="top-link">
                            <li><a href="#aboutus">About</a></li>
                            @*<li><a href="#fun-facts">Staff</a></li>*@
                            <li><a href="#contactus">Contact</a></li>
                            @*<li><a href="#">FAQ</a></li>*@
                        </ul>
                        <!-- End Contact -->
                    </div>
                    <div class="col-lg-6 col-md-7 col-12">
                        <!-- Top Contact -->
                        <ul class="top-contact">
                            <li><i class="fa fa-phone"></i>+234 701 234 5678</li>
                            <li><i class="fa fa-envelope"></i><a href="mailto:support@yourmail.com">support@yourmail.com</a></li>
                        </ul>
                        <!-- End Top Contact -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Topbar -->
        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="inner">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-12">
                            <!-- Start Logo -->
                            <div class="logo">
                                <a asp-page="../index" >KADPOLY
                                @*<img src="~/img/logo.png" asp-append-version="true" alt="#">*@
                                </a>
                            </div>
                            <!-- End Logo -->
                            <!-- Mobile Nav -->
                            <div class="mobile-nav"></div>
                            <!-- End Mobile Nav -->
                        </div>
                        <div class="col-lg-7 col-md-9 col-12">
                            <!-- Main Menu -->
                            <div class="main-menu">
                                <nav class="navigation">
                                    <ul class="nav menu">
                                        <li class="active">
                                            <a asp-page="../index" >Home</a>
                                        </li>
                                        <li><a href="#fun-facts">Staff </a></li>
                                        @*<li><a href="#services">Services </a></li>*@
                                        <li><a href="#aboutus">About Us </a></li>
                                        @*<li>
                                                <a href="#">Blogs <i class="icofont-rounded-down"></i></a>
                                                <ul class="dropdown">
                                                    <li><a href="blog-single.html">Blog Details</a></li>
                                                </ul>
                                            </li>*@
                                        @if (signInManager.IsSignedIn(User))
                                        {
                                            <li>
                                                <a href="#">LogOut <i class="icofont-rounded-down"></i></a>
                                                <ul class="dropdown">
                                                    <li>
                                                        <form method="post" asp-page="logout" >
                                                            <button type="submit" class="btn-link nav-link justify-content-center">LogOut</button>
                                                        </form>
                                                    </li>
                                                </ul>
                                            </li>
                                            if (User.IsInRole("User"))
                                            {
                                                <li><a asp-page="/admin/index" >Dashboard</a></li>
                                            }

                                        }
                                        else
                                        {
                                            <li><a asp-page="account/login" >LogIn</a></li>
                                        }
                                    </ul>
                                </nav>
                            </div>
                            <!--/ End Main Menu -->
                        </div>
                        <div class="col-lg-2 col-12">
                            <div class="get-quote">
                                <a asp-page="#" class="btn">Contact Us</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Header Inner -->
    </header>
    <!-- End Header Area -->

    <div class="container mt-1 mb-2  vh-100">

        @RenderBody()
    </div>
    <!-- Footer Area -->
    <footer id="footer" class="footer mt-2">
      
        <!-- Copyright -->
        <div class="copyright">
            <div class="container">
