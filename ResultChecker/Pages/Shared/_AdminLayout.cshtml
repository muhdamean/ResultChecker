<!DOCTYPE html>
@inject SignInManager<ApplicationUser> signInManager;
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title</title>
    <environment include="Development">
        @*for development environment*@
        @*<link href="~/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />*@
        <!-- Favicon icon -->
        <link rel="icon" href="~/assets/images/favicon.ico" type="image/x-icon">
        <!-- Google font-->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600" rel="stylesheet">
        <!-- Required Fremwork -->
        <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap/css/bootstrap.min.css">
        <!-- themify-icons line icon -->
        <link rel="stylesheet" type="text/css" href="~/assets/icon/themify-icons/themify-icons.css">
        <link rel="stylesheet" type="text/css" href="~/assets/icon/font-awesome/css/font-awesome.min.css">
        <!-- ico font -->
        <link rel="stylesheet" type="text/css" href="~/assets/icon/icofont/css/icofont.css">
        <!-- Style.css -->
        <link rel="stylesheet" type="text/css" href="~/assets/css/style.css">
        <link rel="stylesheet" type="text/css" href="~/assets/css/jquery.mCustomScrollbar.css">
    </environment>
    <environment exclude="Development">
        <!-- Favicon icon -->
        <link rel="icon" href="~/assets/images/favicon.ico" type="image/x-icon">
        <!-- Google font-->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600" rel="stylesheet">
        <!-- Required Fremwork -->
        <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap/css/bootstrap.min.css">
        <!-- themify-icons line icon -->
        <link rel="stylesheet" type="text/css" href="~/assets/icon/themify-icons/themify-icons.css">
        <link rel="stylesheet" type="text/css" href="~/assets/icon/font-awesome/css/font-awesome.min.css">
        <!-- ico font -->
        <link rel="stylesheet" type="text/css" href="~/assets/icon/icofont/css/icofont.css">
        <!-- Style.css -->
        <link rel="stylesheet" type="text/css" href="~/assets/css/style.css">
        <link rel="stylesheet" type="text/css" href="~/assets/css/jquery.mCustomScrollbar.css">
    </environment>
</head>
<body>
    <!-- Pre-loader start -->
    <div class="theme-loader">
        <div class="loader-track">
            <div class="loader-bar"></div>
        </div>
    </div>
    <!-- Pre-loader end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">

            <nav class="navbar header-navbar pcoded-header">
                <div class="navbar-wrapper">
                    <div class="navbar-logo">
                        <a class="mobile-menu" id="mobile-collapse" href="#!">
                            <i class="ti-menu"></i>
                        </a>
                        <div class="mobile-search">
                            <div class="header-search">
                                <div class="main-search morphsearch-search">
                                    <div class="input-group">
                                        <span class="input-group-addon search-close"><i class="ti-close"></i></span>
                                        <input type="text" class="form-control" placeholder="Enter Keyword">
                                        <span class="input-group-addon search-btn"><i class="ti-search"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a asp-page="/index" >Result Checker
                            @*<img class="img-fluid" src="~/assets/images/logo.png" alt="Theme-Logo" />*@
                        </a>
                        <a class="mobile-options">
                            <i class="ti-more"></i>
                        </a>
                    </div>

                    <div class="navbar-container container-fluid">
                        <ul class="nav-left">
                            <li>
                                <div class="sidebar_toggle"><a href="javascript:void(0)"><i class="ti-menu"></i></a></div>
                            </li>
                          
                            <li>
                                <a href="#!" onclick="javascript:toggleFullScreen()">
                                    <i class="ti-fullscreen"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="nav-right">
                            <li class="header-notification">
                                <a href="#!">
                                    <i class="ti-bell"></i>
                                    <span class="badge bg-c-pink"></span>
                                </a>
                                <ul class="show-notification">
                                    <li>
                                        <h6>Notifications</h6>
                                        <label class="label label-danger">New</label>
                                    </li>
                                    <li>
                                        <div class="media">
                                            <div class="media-body">
                                                <a  href="#" >
                                                    <h5 class="notification-user">View </h5>
                                                    <p class="notification-msg">result uploaded available here as its submitted.</p>
                                                    <span class="notification-time">recents uploads</span>
                                                </a>
                                            </div>
                                        </div>
                                    </li>
                                 
                                </ul>
                            </li>

                            <li class="user-profile header-notification">
                                <a href="#!">
                                    <img src="~/img/sorry-no-image-available.jpg" asp-append-version="true" id="staffImage" height="45" width="45" class="img-radius" alt="Profile-Image">
                                    <span><label id="staffName"></label></span>
                                    <i class="ti-angle-down"></i>
                                </a>
                                <ul class="show-notification profile-notification">
                                    <li>
                                        <a asp-page="/index" >
                                            <i class="ti-settings"></i> Home Page
                                        </a>
                                    </li>
                                    <li>
                                        <a asp-page="/admin/staffprofile" >
                                            <i class="ti-user"></i> Profile
                                        </a>
                                    </li>

                                    <li>
                                        <a asp-page="/admin/changepassword">
                                            <i class="ti-lock"></i>Change Password
                                        </a>
                                    </li>
                                    @if (signInManager.IsSignedIn(User))
                                    {
                                        <li>
                                            <form method="post" asp-page="index" >
                                                <button type="submit" class="btn-link">
                                                    <i class="ti-layout-sidebar-left"></i> Logout
                                                </button>
                                            </form>

                                        </li>
                                    }
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
