                            <ul class="pcoded-item pcoded-left-item">
                                <li>
                                    <a asp-page="/admin/courseLecturers">
                                        <span class="pcoded-micon"><i class="ti-layers"></i><b>CL</b></span>
                                        <span class="pcoded-mtext" data-i18n="nav.form-components.main">Course Lecturer</span>
                                        <span class="pcoded-mcaret"></span>
                                    </a>
                                </li>
                            </ul>
                            <div class="pcoded-navigatio-lavel" data-i18n="nav.category.other">Students</div>
                            <ul class="pcoded-item pcoded-left-item">
                                <li>
                                    <a asp-page="/student/index">
                                        <span class="pcoded-micon"><i class="ti-layers"></i><b>ST</b></span>
                                        <span class="pcoded-mtext" data-i18n="nav.form-components.main">Students</span>
                                        <span class="pcoded-mcaret"></span>
                                    </a>
                                </li>
